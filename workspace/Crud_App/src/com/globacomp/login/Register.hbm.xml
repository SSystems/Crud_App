<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping package="com.globacomp.login">
	
	<class name="RegisterBean" table="AddUserTable">
		
		<composite-id>
		
		<key-property name= "id"  column="ID"
		                       type = "integer"/>
		</composite-id>
		        <property name="name" 
				column ="Name"/>
				<property name="password" 
				column ="Password"/>
				<property name="email" 
				column ="Email"/>
				<property name="ctc" 
				column ="Ctc"/>		
				
					
	
	
		</class>
</hibernate-mapping>
